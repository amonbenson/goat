{
    "files.associations": {
        "*.c": "c",
        "*.h": "c",
        "*.lds": "linkerscript",
        "*.xaml": "xml",
        "*.pl": "prolog",
        "*.make": "makefile",
        "*.json": "jsonc",
        "Makefile.*": "makefile",
        "random": "c",
        "functional": "c",
        "*.tcc": "c",
        "memory_resource": "c",
        "memory": "c",
        "fstream": "c",
        "istream": "c",
        "ostream": "c",
        "sstream": "c",
        "streambuf": "c",
        "typeinfo": "c",
        "array": "c"
    },
    "editor.rulers": [100],
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "(\\.c|\\.h)$",
                "cmd": "make all"
            },
            {
                "match": "(\\.dox|\\.md)$",
                "cmd": "make docs"
            }
        ]
    }
}
